<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <report
            id="report_evaluate_cards"
            string="Evaluate Card"
            model="hr.evaluate"
            report_type="qweb-pdf"
            name="hr_evaluate.report_evaluate"
            file="hr_evaluate.report_evaluate"
    />


    <template id="report_evaluate">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="hr_evaluate.report_evaluate_card"/>
            </t>
        </t>
    </template>

    <template id="report_evaluate_card">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Evaluate Card</h2>
            </t>
        </t>
    </template>
</odoo>